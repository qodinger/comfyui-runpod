# ComfyUI RunPod Environment Configuration
# Copy this file to .env and update with your actual values

# ComfyUI Server Configuration
COMFYUI_URL=http://localhost:8188
COMFYUI_PORT=8188

# API Authentication (Optional)
# Set this if you want to use API key authentication for handler
COMFYUI_API_KEY=

# Generation Settings
GENERATION_TIMEOUT=300
POLL_INTERVAL=1.0

# Database Configuration (Optional)
# DATABASE_URL=sqlite:///user/comfyui.db

# Logging
# LOG_LEVEL=INFO
# LOG_STDOUT=false

